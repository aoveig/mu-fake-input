{
  "name": "mu-fake-input",
  "private": false,
  "version": "2.0.0",
  "description": "vue3验证码输入框组件",
  "keywords": [
    "vue3",
    "input",
    "verification code",
    "验证码输入框"
  ],
  "homepage": "https://github.com/aoveig/mu-fake-input",
