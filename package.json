  },
  "devDependencies": {
    "@types/node": "^20.9.4",
    "@vitejs/plugin-vue": "^4.5.0",
    "sass": "^1.69.5",
    "typescript": "^5.8.2",
    "vite": "^5.0.0",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vue-tsc": "^2.2.4"
